<?php

namespace ORMBundle\Entity;

/**
 * LinkGit
 */
class LinkGit
{
    /**
     * @var integer
     */
    private $id_git;

    /**
     * @var integer
     */
    private $id_site;

    /**
     * @var \ORMBundle\Entity\Sites
     */
    private $sites;

    /**
     * @var \ORMBundle\Entity\Git
     */
    private $git;


    /**
     * Set idGit
     *
     * @param integer $idGit
     *
     * @return LinkGit
     */
    public function setIdGit($idGit)
    {
        $this->id_git = $idGit;

        return $this;
    }

    /**
     * Get idGit
     *
     * @return integer
     */
    public function getIdGit()
    {
        return $this->id_git;
    }

    /**
     * Set idSite
     *
     * @param integer $idSite
     *
     * @return LinkGit
     */
    public function setIdSite($idSite)
    {
        $this->id_site = $idSite;

        return $this;
    }

    /**
     * Get idSite
     *
     * @return integer
     */
    public function getIdSite()
    {
        return $this->id_site;
    }

    /**
     * Set sites
     *
     * @param \ORMBundle\Entity\Sites $sites
     *
     * @return LinkGit
     */
    public function setSites(\ORMBundle\Entity\Sites $sites = null)
    {
        $this->sites = $sites;

        return $this;
    }

    /**
     * Get sites
     *
     * @return \ORMBundle\Entity\Sites
     */
    public function getSites()
    {
        return $this->sites;
    }

    /**
     * Set git
     *
     * @param \ORMBundle\Entity\Git $git
     *
     * @return LinkGit
     */
    public function setGit(\ORMBundle\Entity\Git $git = null)
    {
        $this->git = $git;

        return $this;
    }

    /**
     * Get git
     *
     * @return \ORMBundle\Entity\Git
     */
    public function getGit()
    {
        return $this->git;
    }
    /**
     * @var integer
     */
    private $id;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
