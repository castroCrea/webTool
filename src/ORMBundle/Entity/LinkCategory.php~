<?php

namespace ORMBundle\Entity;

/**
 * LinkCategory
 */
class LinkCategory
{
    /**
     * @var integer
     */
    private $id_category;

    /**
     * @var integer
     */
    private $id_site;

    /**
     * @var \ORMBundle\Entity\Sites
     */
    private $sites;

    /**
     * @var \ORMBundle\Entity\Category
     */
    private $category;


    /**
     * Set idCategory
     *
     * @param integer $idCategory
     *
     * @return LinkCategory
     */
    public function setIdCategory($idCategory)
    {
        $this->id_category = $idCategory;

        return $this;
    }

    /**
     * Get idCategory
     *
     * @return integer
     */
    public function getIdCategory()
    {
        return $this->id_category;
    }

    /**
     * Set idSite
     *
     * @param integer $idSite
     *
     * @return LinkCategory
     */
    public function setIdSite($idSite)
    {
        $this->id_site = $idSite;

        return $this;
    }

    /**
     * Get idSite
     *
     * @return integer
     */
    public function getIdSite()
    {
        return $this->id_site;
    }

    /**
     * Set sites
     *
     * @param \ORMBundle\Entity\Sites $sites
     *
     * @return LinkCategory
     */
    public function setSites(\ORMBundle\Entity\Sites $sites = null)
    {
        $this->sites = $sites;

        return $this;
    }

    /**
     * Get sites
     *
     * @return \ORMBundle\Entity\Sites
     */
    public function getSites()
    {
        return $this->sites;
    }

    /**
     * Set category
     *
     * @param \ORMBundle\Entity\Category $category
     *
     * @return LinkCategory
     */
    public function setCategory(\ORMBundle\Entity\Category $category = null)
    {
        $this->category = $category;

        return $this;
    }

    /**
     * Get category
     *
     * @return \ORMBundle\Entity\Category
     */
    public function getCategory()
    {
        return $this->category;
    }
    /**
     * @var integer
     */
    private $id;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
